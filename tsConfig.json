{
    "compilerOptions": {
    "allowSyntheticDefaultImports": true,
      "target": "esnext",
      "module": "esnext",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": false,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx"
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "build", "dist"]
  }
  